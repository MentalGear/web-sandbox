{
	"useTabs": false,
	"singleQuote": true,
	"trailingComma": "es5",
	"comma-dangle": "always-multiline",
	"semi": false,
	"tabWidth": 4,
	"bracketSpacing": true,
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"tailwindStylesheet": "./src/app.css"
}